{
  "name": "cypress-allure",
  "version": "1.0.0",
  "description": "cypress + allure reports setup",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/cypress run --reporter mocha-allure-reporter",
    "report": "./node_modules/.bin/allure generate allure-results --clean -o allure-report",
    "clean": "rm -r allure-results/ allure-report/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkmozgawa/cypress-allure.git"
  },
  "author": "hello@magda.space",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mkmozgawa/cypress-allure/issues"
  },
  "homepage": "https://github.com/mkmozgawa/cypress-allure#readme",
  "devDependencies": {
    "cypress": "^3.4.0"
  },
  "dependencies": {
    "allure-commandline": "^2.9.0",
    "mocha": "^6.2.0",
    "mocha-allure-reporter": "^1.4.0"
  }
}
